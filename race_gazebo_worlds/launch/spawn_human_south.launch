<?xml version="1.0"?>
<launch>

  <arg name="namespace" default=""/>

  <node name="spawn_human_north" pkg="gazebo" type="spawn_model"
    args="rosrun gazebo spawn_model -file $(find race_gazebo_worlds)/models/race_female_standing.model
    -gazebo -model human_north2 -gazebo_namespace $(arg namespace)/gazebo -x -4.55 -y 0.58 -Y 3.14" output="screen"/>

</launch>
